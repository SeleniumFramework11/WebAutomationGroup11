     <?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Netflix Automation Script" parallel="true">

    <parameter name="useCloudEnv" value="false"/>
    <parameter name="cloudEnvName" value="browserstack"/>
    <!--<parameter name="cloudEnvName" value="saucelabs"/>-->
    <parameter name="OS" value="Windows"/>
    <parameter name="os_version" value="10"/>
    <parameter name="browserName" value="Chrome"/>
    <parameter name="browserVersion" value="71.0"/>
    <parameter name="url" value="https://www.netflix.com//"/>

<test name="Search Items">
    <classes>
        <class name="DatabaseTest.DatabaseTest"/>
        <class name="ExcelSheetSearchMovies.TestSearchMoviesXcel"/>
        <class name="GoogleSheetLoginTest.TestSigninWithGoogleSheet"/>
        <class name="TestHomePage.TestHomePage"/>
        <class name="TestHomePage.TestJobsPage"/>
        <class name="TestMainPage.SearchMovies"/>
        <class name="TestMainPage.TestMainPage"/>
        <class name="TestMainPage.TestMainPageFooter"/>
        <class name="TestProfileSelectionPage.TestProfileSelectionPage"/>
        <class name="TestSignInPage.TestSignInPage"/>
        <class name="TestSignInPage.TestSignInUsers"/>
    </classes>
</test>
</suite>
